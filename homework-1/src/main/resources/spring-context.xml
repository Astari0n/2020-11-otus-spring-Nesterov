<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="csvQuestionParser" class="ru.otus.spring.parsers.QuestionParserCsv">
    <constructor-arg value="questions.csv"/>
  </bean>

  <bean id="consoleInteractor" class="ru.otus.spring.interactors.ConsoleInteractor">
    <constructor-arg ref="csvQuestionParser"/>
  </bean>

</beans>
